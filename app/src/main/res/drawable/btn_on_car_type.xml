<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <solid android:color="@color/colorLighterBlack"/>
            <size
                android:width="74dp"
                android:height="36dp" />
        </shape>
    </item>
    <item
        android:top="9dp"
        android:bottom="9dp"
        android:left="28dp"
        android:right="28dp">
        <shape android:shape="oval">
            <solid android:color="@color/colorLighterBlack" />
            <stroke
                android:width="3dp"
                android:color="@android:color/white" />
        </shape>
    </item>
</layer-list>
